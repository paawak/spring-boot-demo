<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd"
	logicalFilePath="1__bank_details_initial_ddl.xml">
	<changeSet author="Palash Ray" id="1">

		<createTable tableName="bank_details">
			<column name="id" type="INT">
				<constraints primaryKey="true"
					primaryKeyName="bank_details_id_PK" />
			</column>
			<column name="age" type="BIGINT" />
			<column name="job" type="VARCHAR(50)" />
			<column name="marital" type="VARCHAR(50)" />
			<column name="education" type="VARCHAR(50)" />
			<column name="defaulted" type="VARCHAR(50)" />
			<column name="balance" type="BIGINT" />
			<column name="housing" type="VARCHAR(3)" />
			<column name="loan" type="VARCHAR(3)" />
			<column name="contact" type="VARCHAR(50)" />
			<column name="day" type="INT" />
			<column name="month" type="VARCHAR(50)" />
			<column name="duration" type="INT" />
			<column name="campaign" type="INT" />
			<column name="pdays" type="INT" />
			<column name="previous" type="INT" />
			<column name="poutcome" type="VARCHAR(50)" />
			<column name="y" type="VARCHAR(3)" />
		</createTable>

	</changeSet>
</databaseChangeLog>
